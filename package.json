{
  "name": "NMI",
  "version": "0.0.1",
  "private": true,
  "description": "NMI web based application",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "deploy": "npm run site && npm run gh-pages",
    "dev": "npm run start:dev",
    "fetch:blocks": "pro fetch-blocks --branch=umi@3 && npm run prettier",
    "gh-pages": "cp CNAME ./dist/ && gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "start": "umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/compatible": "1.0.5",
    "@ant-design/icons": "4.2.2",
    "@ant-design/pro-layout": "6.4.14",
    "@ant-design/pro-table": "2.6.3",
    "@antv/data-set": "0.11.5",
    "@antv/l7": "2.2.34",
    "@antv/l7-maps": "2.2.34",
    "@types/lodash.debounce": "4.0.6",
    "@types/lodash.isequal": "4.5.5",
    "antd": "4.6.2",
    "array-move": "^3.0.1",
    "bizcharts": "^3.5.9",
    "bizcharts-plugin-slider": "^2.1.1-beta.1",
    "classnames": "2.2.6",
    "dva": "2.4.1",
    "gg-editor": "^2.0.2",
    "html2canvas": "^1.0.0-rc.7",
    "js-sha256": "^0.9.0",
    "jspdf": "^2.3.1",
    "lodash": "4.17.20",
    "lodash-decorators": "6.0.1",
    "lodash.debounce": "4.0.8",
    "lodash.isequal": "4.5.0",
    "mockjs": "1.1.0",
    "moment": "2.27.0",
    "numeral": "2.0.6",
    "nzh": "1.0.4",
    "omit.js": "1.0.2",
    "path-to-regexp": "2.4.0",
    "prop-types": "15.7.2",
    "qs": "6.9.4",
    "react": "^16.9.48",
    "react-device-detect": "^1.15.0",
    "react-dom": "^16.13.1",
    "react-fittext": "^1.0.0",
    "react-helmet-async": "^1.0.6",
    "react-highlight-words": "^0.16.0",
    "react-leaf-carousel": "^2.0.3",
    "react-multi-carousel": "^2.5.5",
    "react-player": "^2.7.0",
    "react-router": "^4.3.1",
    "react-scroll": "^1.8.1",
    "react-select": "^3.1.0",
    "react-smooth-dnd": "^0.11.1",
    "react-sortable-hoc": "^2.0.0",
    "socket.io": "1.7.3",
    "umi": "3.2.19",
    "umi-request": "1.3.5",
    "use-merge-value": "1.0.2"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "1.0.22",
    "@types/classnames": "2.2.10",
    "@types/express": "4.17.7",
    "@types/history": "4.7.2",
    "@types/jest": "25.2.3",
    "@types/lodash": "4.14.161",
    "@types/qs": "6.9.4",
    "@types/react": "^16.9.48",
    "@types/react-dom": "16.9.8",
    "@types/react-helmet": "^5.0.16",
    "@umijs/fabric": "2.2.2",
    "@umijs/plugin-blocks": "2.2.1",
    "@umijs/preset-ant-design-pro": "1.2.2",
    "@umijs/preset-react": "1.6.4",
    "@umijs/preset-ui": "2.2.7",
    "carlo": "0.9.46",
    "chalk": "4.1.0",
    "cross-env": "7.0.2",
    "cross-port-killer": "1.2.1",
    "detect-installer": "1.0.2",
    "enzyme": "3.11.0",
    "eslint": "6.8.0",
    "express": "4.17.1",
    "gh-pages": "2.2.0",
    "husky": "4.2.5",
    "jsdom-global": "3.0.2",
    "lint-staged": "10.2.13",
    "mockjs": "1.1.0",
    "prettier": "2.0.5",
    "pro-download": "1.0.1",
    "puppeteer-core": "2.1.1",
    "stylelint": "13.7.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
